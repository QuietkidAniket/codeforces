[
    {
        "command": "runCommands",
        "key": "f5",
        "args" :  {
            "commands": [
                {"command": "workbench.action.files.save"},
                {"command": "workbench.action.terminal.toggleTerminal"},
                {"command": "workbench.action.terminal.focus"},
                {"command": "workbench.action.terminal.sendSequence",  "args": {"text": "~/ccpp.zsh '${file}' \u000D"} },
                // {"command": "workbench.action.terminal.sendSequence",  "args": {"text": "/.${fileDirname}/${fileBasenameNoExtension} \u000D "}},
            ]

        }
    }
]



